<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.naresh.paseo.Main2Activity"
    tools:showIn="@layout/app_bar_main2"
    android:background="#3a4c56">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Current Trip Details"
        android:id="@+id/textView5"
        android:layout_above="@+id/textView6"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="71dp"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="There are no current Trip Details !!"
        android:id="@+id/textView6"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/textView4"
        android:textSize="35dp"
        android:paddingTop="20dp"
        android:textColor="#ffffff"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Welcome to Paseo"
        android:id="@+id/textView7"
        android:layout_marginTop="18dp"
        android:textColor="#ffffff"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Emergency"
        android:id="@+id/button2"
        android:layout_marginTop="85dp"
        android:layout_below="@+id/textView6"
        android:layout_centerHorizontal="true"
        android:onClick="Emergency"
        android:background="#f30707"
        android:textColor="#ffffff" />
<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <ImageButton
        android:id="@+id/i1"
        android:layout_width="wrap_content"
        android:background="#3a4c56"
        android:layout_height="wrap_content"
        android:src="@drawable/night"
        android:layout_marginTop="420dp"
        android:layout_marginLeft="20dp"
        android:onClick="clickFeed"
        android:tag="1"
        />

    <ImageButton
        android:id="@+id/i2"
        android:layout_width="wrap_content"
        android:background="#3a4c56"
        android:layout_height="wrap_content"
        android:src="@drawable/night"
        android:layout_marginTop="420dp"
        android:layout_marginLeft="30dp"
        android:onClick="clickFeed"
        android:tag="2"
        />
    <ImageButton
        android:id="@+id/i3"
        android:layout_width="wrap_content"
        android:background="#3a4c56"
        android:layout_height="wrap_content"
        android:src="@drawable/night"
        android:layout_marginTop="420dp"
        android:layout_marginLeft="40dp"
        android:onClick="clickFeed"
        android:tag="3"
        />
    <ImageButton
        android:id="@+id/i4"
        android:layout_width="wrap_content"
        android:background="#3a4c56"
        android:layout_height="wrap_content"
        android:src="@drawable/night"
        android:layout_marginTop="420dp"
        android:layout_marginLeft="40dp"
        android:onClick="clickFeed"
        android:tag="4"
        />
    <ImageButton
        android:id="@+id/i5"
        android:layout_width="wrap_content"
        android:background="#3a4c56"
        android:layout_height="wrap_content"
        android:src="@drawable/night"
        android:layout_marginTop="420dp"
        android:layout_marginLeft="40dp"
        android:onClick="clickFeed"
        android:tag="5"
        />

</LinearLayout>
</RelativeLayout>
